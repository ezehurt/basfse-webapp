{
  "hosting": {
    "public": "dist/basfse-webapp",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "headers": [ {
    "source" : "**",
    "headers" : [ {
      "key" : "Access-Control-Allow-Origin",
      "value" : "*"
    } ]
  }],
  "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
}
